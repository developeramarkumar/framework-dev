<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{% block title %}Welcome!{% endblock %}</title>

    <!-- Base Styles -->
    {% stylesheets  
        '@AdminBundle/Resources/public/css/bootstrap.min.css'
        '@AdminBundle/Resources/public/css/bootstrap-reset.css'
        '@AdminBundle/Resources/public/css/style.css'
        filter="uglifycss" 
        filter='cssrewrite'
        output="auth.css"
       
    %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="js/html5shiv.min.js"></script>
    <script src="js/respond.min.js"></script>
    <![endif]-->


</head>

    <body class="login-body">
        {% block body %}{% endblock %}
    </body>
    {% javascripts  '@AdminBundle/Resources/public/js/jquery-1.11.1.min.js'
                    '@AdminBundle/Resources/public/js/bootstrap.min.js' filter="uglifyjs2"  output="js/auth.js" %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %} 
</html>